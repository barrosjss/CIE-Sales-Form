{
  "buildCommand": null,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/certificacion",
      "destination": "/formulario-certificacion.html"
    },
    {
      "source": "/consultoria",
      "destination": "/formulario-consultoria.html"
    },
    {
      "source": "/formacion",
      "destination": "/formulario-formacion.html"
    },
    {
      "source": "/licencias",
      "destination": "/formulario-licencias.html"
    },
    {
      "source": "/colaborar",
      "destination": "/formulario-colaborar.html"
    },
    {
      "source": "/otros",
      "destination": "/formulario-otros.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
